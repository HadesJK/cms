<div class="box-content">
    <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
        <thead>
        <tr>
            <th>序号</th>
            <th>说略图</th>
            <th>预测结果</th>
            <th>预测概率</th>
        </tr>
        </thead>
        <tbody>
            #foreach($cm in $ct)
            <tr>
                <td>$!cm.id</td>
                <td>$date.format('yyyy-MM-dd, hh:mm:ss',$!cm.buildTime) </td>
                <td>$!cm.patient</td>
                <td class="center">$!cm.hospital</td>
            </tr>
            #end
        </tbody>
    </table>
</div>