<div class="box-content">
    <table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
        <thead>
        <tr>
            <th>序号</th>
            <th>缩略图</th>
            <th>预测结果</th>
            <th>预测概率</th>
        </tr>
        </thead>
        <tbody>
        #foreach($pred in $preds)
        <tr>
            <td>$!pred.serialNum</td>
            <td>$!pred.pic</td>
            <td>$!pred.label</td>
            <td>$!pred.probably</td>
        </tr>
        #end
        </tbody>
    </table>
</div>